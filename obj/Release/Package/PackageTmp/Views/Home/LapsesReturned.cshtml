



@model IEnumerable<Test__Connection.Models.DateRangeLapseTEST_Result>

@{
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
}


<br /><br />
<table class="table">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.RenewalDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Telephone)
        </th>*@
     
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td >
            @Html.DisplayFor(modelItem => item.RenewalDate)
        </td>
        <td class="refMain">
            @Html.DisplayFor(modelItem => item.Reference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Telephone)
        </td>*@
        <td>
            @Html.ActionLink("Call", "Index", "CallScreen", new { reference = item.Reference, renewalDate = item.RenewalDate,
           telephone = item.Telephone, name = item.Name},
            new { @class = "btn btn-primary", target = "_blank" })
        </td>

        
    </tr>
    }

</table>


